{
  "imports": [],
  "groups": [
    {
      "name": "servers",
      "children": [
        "consul",
        "nats",
        "microweb"
      ]
    }
  ],
  "services": [
    {
      "name": "consul",
      "path": "/usr/local/bin",
      "commands": {
        "launch": "consul agent -dev"
      }
    },
    {
      "name": "microweb",
      "commands": {
        "launch": "micro web"
      }
    },
    {
      "name": "nats",
      "commands": {
        "launch": "gnatsd"
      }
    },
    {
      "name": "nats-streaming",
      "commands": {
        "launch": "nats-streaming-server"
      }
    },
    {
      "name": "gorillaweb",
      "path": "/home/jian/go/src/github.com/jianhan/gorillaweb/api",
      "commands": {
        "build": "go build -o ../bin/gorillaweb",
        "launch": "../bin/gorillaweb"
      },
      "env": [],
      "watch": {}
    },
    {
      "name": "course-management-service",
      "path": "/home/jian/go/src/github.com/jianhan/course-management-service",
      "commands": {
        "build": "go build -o ./bin/course-management-service",
        "launch": "./bin/course-management-service"
      },
      "env": [
        "ENV=development"
      ],
      "watch": {}
    }
  ]
}